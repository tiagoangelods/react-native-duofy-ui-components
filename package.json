{
  "name": "react-native-duofy-ui-components",
  "version": "1.0.54",
  "description": "React Native Duofy UI Components",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "lint": "eslint \"./src/**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "pods": "pod-install --quiet",
    "bootstrap": "yarn && yarn pods",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher",
    "prepare": "rm -rf lib && pinst --disable && bob build && pinst --enable"
  },
  "keywords": [
    "react-native",
    "react-native-duofy-ui-components",
    "react-native-component",
    "ios",
    "android"
  ],
  "repository": "https://github.com/tiagoangelods/react-native-duofy-ui-components",
  "author": "Tiago Angelo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiagoangelods/react-native-duofy-ui-components/issues"
  },
  "homepage": "https://github.com/tiagoangelods/react-native-duofy-ui-components#readme",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@commitlint/config-conventional": "^8.3.4",
    "@heroicons/react": "^2.0.18",
    "@jest/globals": "^29.6.1",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-native-community/bob": "^0.16.2",
    "@react-native-community/datetimepicker": "^7.4.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/slider": "^4.4.2",
    "@react-native/metro-config": "^0.72.9",
    "@release-it/conventional-changelog": "^1.1.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-ondevice-actions": "^6.5.4",
    "@storybook/addon-ondevice-controls": "^6.5.4",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/react-native": "^6.5.4",
    "@testing-library/react-native": "^12.1.3",
    "@types/jest": "^26.0.0",
    "@types/react": "^18.2.15",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-loader": "^8.3.0",
    "commitlint": "^8.3.5",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "nativewind": "^2.0.11",
    "pinst": "^3.0.0",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.3",
    "react-native-heroicons": "^3.2.0",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-svg": "^13.10.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "^18.2.0",
    "release-it": "^13.5.8",
    "tailwindcss": "^3.3.3",
    "twrnc": "^3.6.3",
    "typescript": "^5.1.6"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "printWidth": 88,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "peerDependencies": {
    "nativewind": "^2.0.11",
    "react": "^18.2.0",
    "react-native": "^0.72.3",
    "react-native-heroicons": "^3.2.0",
    "twrnc": "^3.6.3"
  }
}
